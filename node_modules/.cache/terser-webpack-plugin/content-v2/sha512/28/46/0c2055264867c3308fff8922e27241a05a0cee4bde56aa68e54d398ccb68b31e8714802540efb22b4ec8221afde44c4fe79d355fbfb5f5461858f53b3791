omponents/all\";\r\nimport {useSelector, useDispatch} from \"react-redux\";\r\nfunction App() {\r\n  const dispatch = useDispatch();\r\n\r\n  const enterAs = useSelector(state => state.enterAs)\r\n\r\n  const { token, login, logout, EmployeeId } = useAuth();\r\n  let routes;\r\n\r\n  if (token) {\r\n\r\n    if (enterAs !== \"\") {\r\n      routes = <All />;\r\n    } else {\r\n      if (EmployeeId.typeEmployee === \"administrateur\") {       \r\n        routes = <ConnectAdmin />;\r\n      } else if (EmployeeId.typeEmployee === \"Superviseur\") {\r\n        routes = <ConnectSup />;\r\n      } else if (EmployeeId.typeEmployee === \"Coach\") {\r\n        dispatch({type : 'coach'});\r\n        routes = <All />;\r\n      }\r\n    }\r\n  } else routes = <LogIn />;\r\n\r\n  return (\r\n    <div>\r\n      <AuthContext.Provider\r\n        value={{\r\n          isLoggedIn: !!token,\r\n          token: token,\r\n          EmployeeId: EmployeeId,\r\n          login: login,\r\n          logout: logout,\r\n        }}\r\n      >\r\n        <main>{routes}</main>\r\n      </AuthContext.Provider>\r\n    </div>\r\n  );\r\n}\r\nexport default App;\r\n","import { createStore } from \"redux\";\r\nconst enterTypeReducer = (state = { enterAs: \"\" }, action) => {\r\n  if (action.type===\"coach\") {\r\n    return {\r\n      enterAs: \"coach\",\r\n    };\r\n  } else if (action.type===\"sup\") {\r\n    return {\r\n      enterAs: \"sup\",\r\n    };\r\n  } else if (action.type===\"admin\") {\r\n    return {\r\n      enterAs: \"admin\",\r\n    };\r\n  } else return state ;\r\n};\r\nconst store = createStore(enterTypeReducer);\r\nexport default store ;\r\n","import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport App from \"./app\";\nimport { Provider } from \"react-redux\";\nimport store from \"./store/enterTypeReducer\"\nimport { BrowserRouter as Router } from \"react-router-dom\";\nReactDOM.render(\n  <Provider store={store}><Router>\n  <App />\n</Router></Provider>\n  ,\n  do